{
    "name": "logly",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "tsc --noEmit && vite build",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit --pretty",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier -w .",
        "format:check": "prettier -c .",
        "prepare": "husky"
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@mui/icons-material": "^6.0.2",
        "@mui/material": "^6.0.2",
        "@mui/x-charts": "^8.11.1",
        "@mui/x-date-pickers": "^8.10.2",
        "dayjs": "^1.11.13",
        "firebase": "^10.12.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@types/react": "^18.3.7",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.3.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.6.2",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.41.0",
        "vite": "^5.4.1",
        "vite-plugin-pwa": "^0.20.5"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier -w"
        ],
        "*.{css,md,json}": [
            "prettier -w"
        ]
    }
}
